{
  "name": "barracuda-pictures",
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "0.41.0"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": ">=14.17 <=16.x",
    "yarn": ">=1.15 <2"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "scripts": {
    "start": "yarn rw dev",
    "test": "yarn rw test api",
    "dockers": "docker-compose up -d",
    "stop-and-clean": "docker-compose down -v",
    "reset-migrations": "docker-compose down -v && rm -rf ./api/db/migrations && yarn rw migrate dev --name init",
    "scan": "yarn rw exec scan",
    "static": "yarn rw exec static-server",
    "migrate": "yarn rw prisma migrate dev",
    "studio": "yarn rw prisma studio"
  }
}
