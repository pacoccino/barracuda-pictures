{
  "name": "barracuda-pictures",
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "0.41.0"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": ">=14.17 <=16.x",
    "yarn": ">=1.15 <2"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "scripts": {
    "start": "yarn rw dev",
    "test": "yarn rw test api",
    "dc:dev": "docker-compose --env-file .env -f docker-compose.yml -f docker-compose.dev.yml",
    "dc:dev:clean": "yarn dc:dev down -v --remove-orphans",
    "dc:dev:tasks": "yarn dc:dev -f docker-compose.tasks.yml ",
    "dc:prod": "docker-compose --env-file .env.prod --env-file .env.prod -f docker-compose.yml -f docker-compose.prod.yml",
    "dc:prod:tasks": "yarn dc:prod -f docker-compose.tasks.yml ",
    "dc:prod:clean": "yarn dc:prod down -v --remove-orphans",
    "worker": "yarn rw exec worker",
    "scan": "yarn rw exec scan",
    "upload": "yarn rw exec upload",
    "migrate": "yarn rw prisma migrate dev",
    "studio": "yarn rw prisma studio"
  }
}
